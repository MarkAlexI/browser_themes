{
  "name": "Galactic Focus",
  "version": "1.0",
  "description": "Galactic Focus is a theme for Google Chrome that takes you into deep space with an emphasis on concentration.",
  "manifest_version": 3,
  "theme": {
    "images": {
      "theme_frame": "images\/theme_frame.png",
      "theme_toolbar": "images\/theme_toolbar.png",
      "theme_tab_background": "images\/theme_tab_background.png",
      "theme_ntp_background": "images\/theme_ntp_background.jpeg"
    },
    "colors": {
      "frame": [10, 10, 50],
      "toolbar": [134, 174, 194],
      "tab_text": [10, 10, 50],
      "tab_background_text": [134, 174, 194],
      "bookmark_text": [10, 10, 40],
      "ntp_background": [255, 255, 255],
      "ntp_text": [50, 50, 50],
      "ntp_link": [6, 55, 116],
      "button_background": [0, 0, 0, 0]
    },
    "tints": {
      "buttons": [0.67, 0.42, 0.13]
    },
    "properties": {
      "ntp_background_alignment": "bottom",
      "ntp_background_repeat": "no-repeat"
    }
  }
}